{
  "format": "OpenPrefabJSON",
  "version": "1.0",
  "elementId": "uuid", // z. B. "0f12c5b0-1f6e-4c56-9d3c-8e9c0214e2a7"
  "elementNumber": "string", // z. B. "1" oder "Typ A"
  "description": "string", // z. B. "HFT-Sturz, Position 31a“
  "classification": { // Produkttyp, Normen, DIN
    "type": "string", // z. B. "Wall", "Slab", "Stair", "Balcony", "Beam",
    "subtype": "string", // z. B. "HollowCoreSlab", "Straight", "HalfTurned"
    "concreteQuality": "string", // Betongüte
    "standard": "string", // Normung
    "revision": "string" // Revisionsnummer oder Zeichnungsindex 
  },
  "physicalProperties": {
    "length": "number", // Länge des Bauteils (Bounding Box)
    "width": "number", // Breite des Bauteils (Bounding Box)
    "height": "number", // Höhe des Bauteils (Bounding Box)
    "volume": "number" // Volumen des Bauteils 
  },
  "project": {
    "projectId": "string", // Projekt ID, wie sie das ERP erfordert
    "name": "string", // Name des Bauvorhabens
    "street": "string", // Straße des Bauvorhabens
    "postalCode": "string", // Postleitzahl des Bauvorhabens
    "city": "string", // Stadt des Bauvorhabens
    "buildingSection": "string", // Gebäudeinformation, Haus 1, Haus A o.ä.
    "floor": "string" // z.B. 1.OG
  },
  "erpMappings": {
    "articleNumber": "string", // Artikelnummer des Elements
    "costCenter": "string", // Kostenstelle
    "amount": "number", // Menge
    "unit": "string" // Einheit des Artikels, z.B. m, m², Stk.
  },
  "metadata": {
    "createdBy": "string", // z.B "Planungssoftware X"
    "createdAt": "string", // ISO 8601, z.B. "2025-06-05T23:45:00Z",
    "lastModified": "string", // ISO 8601, z.B. "2025-06-06T09:00:00Z",
    "author": "string" // Name des Erstellers
  }
}